{
  "name": "a1c-infra",
  "version": "0.1.0",
  "bin": {
    "a1c-infra": "bin/a1c-infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:full": "cdk deploy",
    "deploy:eks-only": "cdk deploy --context eks-only=true",
    "deploy:eks-existing-vpc": "cdk deploy --context eks-only=true --context vpc-id=vpc-015016081a2494f0e",
    "deploy:helm": "cdk deploy --context eks-only=true --context update-helm=true --context vpc-id=vpc-015016081a2494f0e",
    "deploy:addons": "cdk deploy --context eks-only=true --context update-addons=true --context vpc-id=vpc-015016081a2494f0e"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1016.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-cdk/lambda-layer-kubectl-v32": "^2.1.0",
    "aws-cdk-lib": "2.196.0",
    "constructs": "^10.0.0"
  }
}
